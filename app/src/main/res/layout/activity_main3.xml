<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">

    <TextView
        android:id="@+id/StudentName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:text="Student Sabaq"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Csbaq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:text="Sabaq Para"
        app:layout_constraintBaseline_toBaselineOf="@+id/Csbaqi"
        app:layout_constraintEnd_toStartOf="@+id/Csbaqi"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/Csbaqi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="59dp"
        android:text="Sabqi Para"
        app:layout_constraintBottom_toTopOf="@+id/Recordbtn"
        app:layout_constraintEnd_toStartOf="@+id/Cmanzil"
        app:layout_constraintStart_toEndOf="@+id/Csbaq" />

    <TextView
        android:id="@+id/Cmanzil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:text="Manzil Para"
        app:layout_constraintBaseline_toBaselineOf="@+id/Csbaqi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Csbaqi" />

    <Button
        android:id="@+id/Recordbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="All Students Sabaq"
        app:layout_constraintBaseline_toBaselineOf="@+id/AddNew"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="30dp"
        android:text="Update Sabaq"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/Usbaq"
        app:layout_constraintEnd_toStartOf="@+id/Cmanzil"
        app:layout_constraintStart_toStartOf="@+id/Usbaq" />

    <EditText
        android:id="@+id/Usbaq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Sabaq Para No"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Usbqi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="54dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Sabqi Para No"
        app:layout_constraintStart_toStartOf="@+id/Usbaq"
        app:layout_constraintTop_toBottomOf="@+id/Usbaq" />

    <EditText
        android:id="@+id/Umanzil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Manzil Para No"
        app:layout_constraintBottom_toTopOf="@+id/Updatebtn"
        app:layout_constraintStart_toStartOf="@+id/Usbqi" />

    <Button
        android:id="@+id/Updatebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="46dp"
        android:layout_marginBottom="63dp"
        android:text="Update"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/Umanzil" />

    <Button
        android:id="@+id/AddNew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginBottom="36dp"
        android:text="Add New Record"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/Recordbtn" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Sabaq"
        app:layout_constraintBottom_toTopOf="@+id/Csbaq"
        app:layout_constraintEnd_toEndOf="@+id/Csbaq" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Sabqi"
        app:layout_constraintBottom_toTopOf="@+id/Csbaqi"
        app:layout_constraintEnd_toEndOf="@+id/Csbaqi" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="16dp"
        android:text="Manzil"
        app:layout_constraintBottom_toTopOf="@+id/Cmanzil"
        app:layout_constraintEnd_toEndOf="@+id/Cmanzil" />

</androidx.constraintlayout.widget.ConstraintLayout>